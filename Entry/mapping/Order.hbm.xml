<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Entry" namespace="Entry.model" default-lazy="true">
  <class name="Order" table="Order">
    <id name="OrderId">
      <column name="OrderId" sql-type="char(32)" not-null="true"/>
      <generator class="uuid.hex"/>
    </id>
    <property name="OrderDate" column="OrderDate"  not-null="true"/>
    <many-to-one name="User" column="UserId" cascade="save-update">
      
    </many-to-one>
  </class>
</hibernate-mapping>