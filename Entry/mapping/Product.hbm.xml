<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Entry" namespace="Entry.model" default-lazy="true">
  <class name="Product" table="Product">
    <id name="ProductId" type="Guid">
      <column name="ProductId" sql-type="uniqueidentifier" not-null="true" unique="true"/>
      <generator class="assigned"></generator>
    </id> 
    <property name="Name">
      <column name="Name" sql-type="nvarchar" length="100" not-null="true"></column>
    </property>
    <property name="Version">
      <column name="Version" sql-type="nvarchar" length="100"></column>
    </property>
    <property name="Price">
      <column name="Price" sql-type="float"></column>
    </property>   
    <set name="Orders" table="OrderProduct" lazy="true">
      <key column="ProductId" foreign-key="fk_product_orderproduct"></key>
      <many-to-many column="OrderId" class="Order" foreign-key="fk_order_orderproduct"></many-to-many>
    </set>
  </class>
</hibernate-mapping>